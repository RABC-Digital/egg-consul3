{
  "name": "egg-consul3",
  "version": "1.0.3",
  "description": "Consul plugin for Egg.js",
  "eggPlugin": {
    "name": "consul"
  },
  "keywords": [
    "egg",
    "eggPlugin",
    "egg-plugin",
    "consul"
  ],
  "dependencies": {
    "consul": "^1.2.0"
  },
  "devDependencies": {
    "@eggjs/tsconfig": "^1.3.2",
    "@types/consul": "^0.40.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.15.10",
    "egg": "^3",
    "egg-bin": "^6",
    "egg-ci": "^2",
    "egg-mock": "^5",
    "eslint": "^8",
    "eslint-config-egg": "^12",
    "git-contributor": "^2.0.0",
    "tsd": "^0.28.0",
    "typescript": "4"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "contributor": "git-contributor",
    "tsd": "tsd",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov && npm run prepublishOnly",
    "build": "tsc -p ./tsconfig.json",
    "clean": "tsc --build --clean",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "files": [
    "app/**/*.js",
    "config/*.js",
    "lib/*.js",
    "agent.js",
    "app.js",
    "index.d.ts"
  ],
  "ci": {
    "version": "16, 18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RABC-Digital/egg-consul3.git"
  },
  "bugs": {
    "url": "https://github.com/RABC-Digital/egg-consul3/issues"
  },
  "homepage": "https://github.com/RABC-Digital/egg-consul3#readme",
  "author": "猫爪子",
  "license": "MIT"
}
